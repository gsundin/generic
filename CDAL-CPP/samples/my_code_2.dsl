/* A poetic, yet saddening representation of life in CDAL. */

main () { 
  int x=5;
  if (x==5){
  	print("Hello World, I am 5.");
  	x = x + 5;
  }
  if(x==10){
  	print("Hello World, I am 10.")
  	x = x + 10;
  }

  if(x==20){
  	print("Hello World, I am 20.")
  	x = x + 10;
  }

  if(x>0){
  	print ("Hello World, I am old.")
  }
  
}



